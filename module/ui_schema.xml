<?xml version='1.0' encoding='utf-8'?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>FILL THIS IN</h:title>
    <model>
      <instance>
        <faims id="FILL_THIS_IN">
          <!--START: Style-->
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <!--END: Style-->
          <Welcome>
            <Welcome>
              <Logo/>
              <Start/>
            </Welcome>
          </Welcome>
          <Login>
            <Login>
              <Please_select_a_user/>
              <Log_in/>
              <Signup_Guide_1/>
              <Sign_up/>
            </Login>
            <Manual>
              <Manual/>
              <Go_to_Login_Tab/>
            </Manual>
          </Login>
          <Start>
            <Start>
              <Record_New_Mound/>
              <Next_Mound_ID/>
              <My_Last_3_Records/>
            </Start>
            <Search>
              <Colgroup_0>
                <Col_0>
                  <Search_Term/>
                </Col_0>
                <Col_1>
                  <Search_Button/>
                </Col_1>
              </Colgroup_0>
              <Entity_List/>
            </Search>
            <Map>
              <Map/>
              <Center_Me_1/>
            </Map>
          </Start>
          <Mound>
            <Mound_Admin>
              <Validate/>
              <Mound_author/>
              <Mound_timestamp/>
              <Mound_ID/>
              <Mound_Name/>
              <Colgroup_1>
                <Col_1>
                  <Latitude/>
                </Col_1>
                <Col_2>
                  <Longitude/>
                </Col_2>
              </Colgroup_1>
              <Colgroup_2>
                <Col_1>
                  <Northing/>
                </Col_1>
                <Col_2>
                  <Easting/>
                </Col_2>
                <Col_3>
                  <Accuracy/>
                </Col_3>
              </Colgroup_2>
              <Take_From_GPS_1/>
              <Description_of_mound_or_location/>
              <Colgroup_3>
                <Col_1>
                  <Height_Max/>
                  <Diameter_Max/>
                </Col_1>
                <Col_2>
                  <Height_Min/>
                  <Diameter_Min/>
                </Col_2>
              </Colgroup_3>
              <Image/>
              <Image_Button_1/>
            </Mound_Admin>
            <Condition>
              <Landuse_around_Mound/>
              <Landuse_on_top_of_Mound/>
              <Notes_on_shape_and_dimension/>
              <Condition/>
              <Source_of_Impact/>
              <Robbers_trenches/>
              <Notes_on_Condition/>
              <Detail_photo/>
              <Detail_photo_Button_1/>
              <Associated_material/>
              <Associated_features/>
            </Condition>
          </Mound>
        </faims>
      </instance>
      <bind nodeset="/faims/Start/Start/Next_Mound_ID" type="decimal"/>
      <bind nodeset="/faims/Mound/Mound_Admin/Colgroup_3/Col_1/Height_Max" type="decimal"/>
      <bind nodeset="/faims/Mound/Mound_Admin/Colgroup_3/Col_1/Diameter_Max" type="decimal"/>
      <bind nodeset="/faims/Mound/Mound_Admin/Colgroup_3/Col_2/Height_Min" type="decimal"/>
      <bind nodeset="/faims/Mound/Mound_Admin/Colgroup_3/Col_2/Diameter_Min" type="decimal"/>
    </model>
  </h:head>
  <h:body>
    <!--START: Style-->
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <!--END: Style-->
    <group ref="Welcome">
      <label>{Welcome}</label>
      <group ref="Welcome">
        <label>{Welcome}</label>
        <input faims_web="true" ref="Logo">
          <label/>
        </input>
        <trigger ref="Start">
          <label>{Start}</label>
        </trigger>
      </group>
    </group>
    <group ref="Login">
      <label>{Login}</label>
      <group ref="Login">
        <label>{Login}</label>
        <select1 ref="Please_select_a_user" faims_annotation="false" faims_certainty="false">
          <label>{Please_select_a_user}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <trigger ref="Log_in">
          <label>{Log_in}</label>
        </trigger>
        <input faims_web="true" ref="Signup_Guide_1">
          <label/>
        </input>
        <trigger ref="Sign_up">
          <label>{Sign_up}</label>
        </trigger>
      </group>
      <group ref="Manual">
        <label>{Manual}</label>
        <input faims_web="true" ref="Manual">
          <label/>
        </input>
        <trigger ref="Go_to_Login_Tab">
          <label>{Go_to_Login_Tab}</label>
        </trigger>
      </group>
    </group>
    <group ref="Start">
      <label>{Start}</label>
      <group ref="Start" faims_scrollable="false">
        <label>{Start}</label>
        <trigger ref="Record_New_Mound" faims_annotation="false" faims_certainty="false">
          <label>{Record_New_Mound}</label>
        </trigger>
        <input ref="Next_Mound_ID" faims_style_class="required">
          <label>{Next_Mound_ID}</label>
        </input>
        <select1 appearance="compact" ref="My_Last_3_Records">
          <label>{My_Last_3_Records}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
      <group ref="Search" faims_scrollable="false">
        <label>{Search}</label>
        <group ref="Colgroup_0" faims_style="orientation">
          <label/>
          <group ref="Col_0" faims_style="even">
            <label/>
            <input ref="Search_Term">
              <label>{Search_Term}</label>
            </input>
          </group>
          <group ref="Col_1" faims_style="large">
            <label/>
            <trigger ref="Search_Button">
              <label>{Search}</label>
            </trigger>
          </group>
        </group>
        <select1 appearance="compact" ref="Entity_List">
          <label>{Entity_List}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
      <group ref="Map" faims_scrollable="false">
        <label>{Map}</label>
        <input faims_map="true" ref="Map">
          <label>{Map}</label>
        </input>
        <trigger ref="Center_Me_1">
          <label>{Center_Me}</label>
        </trigger>
      </group>
    </group>
    <group ref="Mound" faims_archent_type="Mound">
      <label>{Mound}</label>
      <group ref="Mound_Admin">
        <label>{Mound_Admin}</label>
        <trigger ref="Validate">
          <label>{Validate}</label>
        </trigger>
        <input faims_attribute_name="Mound author" faims_attribute_type="measure" ref="Mound_author" faims_read_only="true" faims_annotation="false" faims_certainty="false">
          <label>{Mound_author}</label>
        </input>
        <input faims_attribute_name="Mound timestamp" faims_attribute_type="measure" ref="Mound_timestamp" faims_read_only="true" faims_annotation="false" faims_certainty="false">
          <label>{Timestamp}</label>
        </input>
        <input faims_attribute_name="Mound ID" faims_attribute_type="measure" ref="Mound_ID" faims_read_only="true" faims_annotation="false" faims_certainty="false">
          <label>{Mound_ID}</label>
        </input>
        <input faims_attribute_name="Mound Name" faims_attribute_type="measure" ref="Mound_Name">
          <label>{Mound_Name}</label>
        </input>
        <group ref="Colgroup_1" faims_style="orientation">
          <label/>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input faims_attribute_name="Latitude" faims_attribute_type="measure" ref="Latitude" faims_read_only="true">
              <label>{Latitude}</label>
            </input>
          </group>
          <group ref="Col_2" faims_style="even">
            <label/>
            <input faims_attribute_name="Longitude" faims_attribute_type="measure" ref="Longitude" faims_read_only="true">
              <label>{Longitude}</label>
            </input>
          </group>
        </group>
        <group ref="Colgroup_2" faims_style="orientation">
          <label/>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input faims_attribute_name="Northing" faims_attribute_type="measure" ref="Northing" faims_read_only="true">
              <label>{Northing}</label>
            </input>
          </group>
          <group ref="Col_2" faims_style="even">
            <label/>
            <input faims_attribute_name="Easting" faims_attribute_type="measure" ref="Easting" faims_read_only="true">
              <label>{Easting}</label>
            </input>
          </group>
          <group ref="Col_3" faims_style="even">
            <label/>
            <input faims_attribute_name="Accuracy" faims_attribute_type="measure" ref="Accuracy" faims_read_only="true">
              <label>{Accuracy}</label>
            </input>
          </group>
        </group>
        <trigger ref="Take_From_GPS_1">
          <label>{Take_From_GPS}</label>
        </trigger>
        <input faims_attribute_name="Description of mound or location" faims_attribute_type="measure" ref="Description_of_mound_or_location">
          <label>{Description_of_mound_or_location}</label>
        </input>
        <group ref="Colgroup_3" faims_style="orientation">
          <label/>
          <group ref="Col_1" faims_style="even">
            <label/>
            <input faims_attribute_name="Height Max" faims_attribute_type="measure" ref="Height_Max" faims_style_class="required">
              <label>{Height_Max__m_}</label>
            </input>
            <input faims_attribute_name="Diameter Max" faims_attribute_type="measure" ref="Diameter_Max" faims_style_class="required">
              <label>{Diameter_Max__m_}</label>
            </input>
          </group>
          <group ref="Col_2" faims_style="even">
            <label/>
            <input faims_attribute_name="Height Min" faims_attribute_type="measure" ref="Height_Min">
              <label>{Height_Min__m_}</label>
            </input>
            <input faims_attribute_name="Diameter Min" faims_attribute_type="measure" ref="Diameter_Min">
              <label>{Diameter_Min__m_}</label>
            </input>
          </group>
        </group>
        <select faims_sync="true" type="camera" faims_attribute_name="Image" faims_attribute_type="measure" ref="Image">
          <label>{Image}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <trigger ref="Image_Button_1">
          <label>{Image}</label>
        </trigger>
      </group>
      <group ref="Condition">
        <label>{Condition}</label>
        <select1 type="image" faims_attribute_name="Landuse around Mound" faims_attribute_type="vocab" ref="Landuse_around_Mound" faims_style_class="required" faims_html_description="true">
          <label>{Landuse_around_Mound}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 type="image" faims_attribute_name="Landuse on top of Mound" faims_attribute_type="vocab" ref="Landuse_on_top_of_Mound" faims_style_class="required" faims_html_description="true">
          <label>{Landuse_on_top_of_Mound}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <input faims_attribute_name="Notes on shape and dimension" faims_attribute_type="measure" ref="Notes_on_shape_and_dimension">
          <label>{Notes_on_shape_and_dimension}</label>
        </input>
        <select1 faims_attribute_name="Condition" faims_attribute_type="vocab" ref="Condition" faims_style_class="required">
          <label>{Condition}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 faims_attribute_name="Source of Impact" faims_attribute_type="vocab" ref="Source_of_Impact">
          <label>{Source_of_Impact}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select faims_attribute_name="Robbers trenches" faims_attribute_type="vocab" ref="Robbers_trenches">
          <label>{Robber_s_trenches_}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <input faims_attribute_name="Notes on Condition" faims_attribute_type="measure" ref="Notes_on_Condition">
          <label>{Notes_on_Condition}</label>
        </input>
        <select faims_sync="true" type="camera" faims_attribute_name="Detail photo" faims_attribute_type="measure" ref="Detail_photo">
          <label>{Detail_photo}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <trigger ref="Detail_photo_Button_1">
          <label>{Detail_photo}</label>
        </trigger>
        <select faims_attribute_name="Associated material" faims_attribute_type="vocab" ref="Associated_material">
          <label>{Associated_material}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select>
        <input faims_attribute_name="Associated features" faims_attribute_type="measure" ref="Associated_features">
          <label>{Associated_features}</label>
        </input>
      </group>
    </group>
  </h:body>
</h:html>
